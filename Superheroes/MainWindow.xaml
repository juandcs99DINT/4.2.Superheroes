<Window x:Class="Superheroes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Superheroes"
        mc:Ignorable="d"
        Title="Superhéroes" Height="700" MinHeight="500" MinWidth="700" Width="1000" Icon="assets/shield.ico">
    <DockPanel Background="LightGreen" LastChildFill="False">
        <TextBlock Margin="10" FontSize="35" DockPanel.Dock="Left" Text="Texto Génerico" FontFamily="Impact" Foreground="White">
            <TextBlock.Effect>
                <DropShadowEffect Color="Black"/>
            </TextBlock.Effect></TextBlock>
        <Image Source="assets/avengers.png" Width="40" Height="40" DockPanel.Dock="Right" VerticalAlignment="Top" Margin="10"></Image>
    </DockPanel>
</Window>
